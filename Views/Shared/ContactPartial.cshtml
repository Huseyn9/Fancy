@model FancyWebsite.ViewModel.View_Model
<div class="col-lg-3 col-md-6">
    <aside class="f_widget contact_widget">
        <div class="f_w_title">
            <h3>Contact Us</h3>
        </div>
        @foreach (var item in Model._contact)
    {
        <a href="#">@item.contact_phone</a>
        <a href="#">@item.contact_email</a>
        <p>@item.contact_address</p>
        <h6>@item.contact_work_time</h6>
}

    </aside>
</div>
